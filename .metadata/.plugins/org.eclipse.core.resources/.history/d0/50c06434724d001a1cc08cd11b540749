
public final class ImmutableClass {
	
	private final int[] myArray;
	
	public ImmutableClass(int[] a) {
		myArray=a.clone();
	}
	
	
	void getArray(){
		System.out.println(myArray);
	}
	
	public static void main(String[] args) {
		int a[]= {2,3};
		ImmutableClass m=new ImmutableClass(a);
		m.getArray();
	}

}
